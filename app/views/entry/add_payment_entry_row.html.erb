<tr id="<%= @pfd.id %>entry"  class="current_entries_row">
	<td id="payment_date"><%= @pfd.payment_date.strftime("%m-%d-%Y") %></td>
	<td id="job_name"><%= Job.find(params[:job_id]).name %></td>
	<td id="destination_name"><%= Destination.find(params[:destination_id]).name %></td>
	<td id="wood_type" type_id="<%= @pfd.wood_type %>"><%= WoodType.find(params[:wood_type]).name %></td>
	<td id="payment_num"><%= @pfd.payment_num %></td>
	<td id="tickets"><%= @pfd.tickets %></td>
	<td id="tonnage"><%= @pfd.tonnage %></td>
	<td id="net_mbf"><%= @pfd.net_mbf %></td>
	<td class="currency" id="total_payment"><%= @ac.give_pennies(@pfd.total_payment) %></td>
	<td><input type="button" value="edit" onclick="show_payment_inputs(<%= @pfd.id %>)"/> 
	<input type="button" value="del" onclick="delete_payment(<%= @pfd.id %>)"></td>
</tr>