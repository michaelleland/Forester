<tr id="<%= @ticket.id %>entry" class="current_entries_row">
	<td id="ticket_number"><%= @ticket.ticket_num] %></td>
	<td id="delivery_date"><%= @ticket.delivery_date] %></td>
	<td id="job_name"><%= shorten params[:job_name] %></td>
	<td id="destination_name"><%= shorten params[:destination_name] %></td>
	<td id="wood_type" type_id="<%= @ticket.wood_type %>"><%= WoodType.find(params[:wood_type]).name %></td>
	<td id="species"><%=  unless @ticket.load_details.first.species_id == 0 then Species.find(@ticket.load_details.first.species_id-1).code else "PU" end %></td>
	<td id="net_mbf"><%= @ticket.net_mbf %></td>
	<td id="tonnage"><%= @ticket.tonnage %></td>
	<td id="load_pay"><%= @ac.give_pennies(@ticket.value)%></td>
	<td><input type="button" value="edit" onclick="show_ticket_inputs(<%= @ticket.id %>)" /> 
	<input type="button" value="del" onclick="delete_ticket(<%= @ticket.id %>)"></td>
</tr>