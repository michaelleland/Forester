<h5>Deductions: </h5>
<div style="float: left; width: 25px;" id="decdution_chks" > 
  <% unless @deductions.length == 0 %>
    <% @deductions.each_with_index do |i,x| %>
      <input type="checkbox" id="decdution_ids" name="deduction_ids[]" value="<%= i.id %>"  />
    <% end %>
  <% end %>
</div>
<div id="deductions_to_receipt_list" style="float: left;width: 287px;background-color: white">
  <% unless @deductions.length == 0 %>
    <% @deductions.each do |i| %>
  <div><%=i.item_data%></div>
    <% end %>
  <% end %>
</div>

<div id="deductions_to_receipt_list_values" >
  <% unless @deductions.length == 0 %>
    <% @deductions.each do |i| %>
  <div><%=" $ #{i.value}"%></div>
    <% end %>
  <% end %>
</div>

<div id="input_row">
  Deduction: <input type="text" size="16" id="deduction_data" name="deduction_data"/>
  Value: $ <input type="text" size="5" id="deduction_value" name="deduction_value"/>
  <%=link_to "Add" ,"#", {:onclick=>"add_deduction($('#deduction_data').val(), $('#deduction_value').val(), $('#job_selector').val(), $('#owner_type_selector').val())"}%>
  
<div id="feedback"></div>
</div>
